using System;

class Program {
  public static void Main (string[] args) {
    
    int n;
    int sum=0;
    for(n = 0; n <= 10; n+=2)
        {
            sum = sum + n;
        }
        Console.WriteLine("Sum is = " + sum);

        do
        {
            n += 2;
        }
        while (n <= 10);

        {
            n += 2;
        }
  }
}
