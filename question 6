using System;

  class Program {
  public static void Main (string[] args) 
  {
    int n;
    int numEven= 0;
    int numOdd = 0;
    Random rnd = new Random();
    
    for (n = 0; n < 100; n++)
    
    {
      int number = rnd.Next();
      if (number % 2 == 0) 
      
      {
        numEven++;
      }
      else 
      {
        numOdd++;
      }
      
    }
    
    Console.WriteLine("Even numbers: " + numEven);
    Console.WriteLine("Odd numbers: " + numOdd);
  }
}
